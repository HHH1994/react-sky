{
  "name": "knight-errant",
  "version": "1.0.0",
  "description": "这是一个从0开始搭建的react组件库，包含了组件，组件测试，组件demo以及说明文档。",
  "main": "index.js",
  "scripts": {
    "clear": "rimraf dist/components",
    "demoDev": "cross-env NODE_ENV=development webpack-dev-server --hot --inline --config ./examples/webpack.demo.js  --mode development",
    "demoBuild": "npm run clear && cross-env NODE_ENV=production webpack --config ./examples/webpack.demo.js --mode production",
    "docsDev": "cross-env NODE_ENV=development webpack-dev-server --hot --inline --config ./docs/webpack.docs.js  --mode development",
    "docsBuild": "npm run clear && cross-env NODE_ENV=production webpack --config ./docs/webpack.docs.js --mode production",
    "componentsDev": "cross-env NODE_ENV=development webpack-dev-server --hot --inline --config ./components/webpack.components.js  --mode development",
    "componentsBuild": "cross-env NODE_ENV=production webpack --config ./components/webpack.components.js --mode production",
    "lib": "babel ./components/src --out-dir ./dist"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lvpangpang/Knight-Errant.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/lvpangpang/Knight-Errant/issues"
  },
  "homepage": "https://github.com/lvpangpang/Knight-Errant#readme",
  "dependencies": {
    "markdown-it": "^10.0.0",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "react-router": "^5.0.1",
    "react-router-dom": "^5.0.1",
    "stylus": "^0.54.7",
    "typescript": "^3.6.3"
  },
  "devDependencies": {
    "@types/react": "^16.9.2",
    "@types/react-dom": "^16.9.0",
    "awesome-typescript-loader": "^5.2.1",
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.4",
    "babel-plugin-dynamic-import-webpack": "^1.1.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "cross-env": "^5.2.1",
    "css-loader": "^3.2.0",
    "file-loader": "^4.2.0",
    "html-webpack-plugin": "^3.2.0",
    "style-loader": "^1.0.0",
    "stylus-loader": "^3.0.2",
    "ts-loader": "^6.1.0",
    "url-loader": "^2.1.0",
    "webpack": "^4.39.3",
    "webpack-cli": "^3.3.8",
    "webpack-dev-server": "^3.8.0",
    "webpack-merge": "^4.2.2"
  }
}
